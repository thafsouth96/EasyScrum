<!-- Contenu de la pop up -->
<div class="modal fade" id="modelCreateProject" role="dialog">
  <div class="modal-dialog modal-lg">
      <!-- Modal content-->

        <div class="panel panel-warning modal-content" id="formCreateProject">
          <div class="panel-heading model-header" id = "titleCreateProject">Créer un projet
            <button type="button" class="close" data-dismiss="modal">&times;</button>
          </div>
            <form id= "formcreateP" method="POST" action="{{path('project_create')}}">
              <div class="panel-body" id="formCreateProjectContent">
              {{form_start(form)}}
                {{ form_widget(form)}}
                <div class="modal-footer">
                  <input type="submit" value="Créer" class="btn btn-warning" id="createBtn"/>
                </div>
              {{ form_end(form) }}
              </div>
          </form>

        </div>
   </div>
</div>
<!-- Fin du contenu de la pop up -->
